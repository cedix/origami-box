  },
  "author": "Francois Karman",
  "dependencies": {
    "@popperjs/core": "^2.9.0",
    "@tippyjs/react": "^4.2.3",
    "bootstrap": "^5.0.0-beta2",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "dayjs": "^1.10.4",
    "i18next": "^19.9.1",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.8",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.3",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "husky": "=4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prelocal:db": "docker-compose down && docker-compose build",
    "local:db": "docker-compose up",
    "postlocal:db": "docker-compose down",
    "local:api": "cd api && func start",
    "local": "concurrently \"yarn local:db\" \"yarn local:api\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:7071",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
